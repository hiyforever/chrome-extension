{
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "run_at": "document_start"
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "contextMenus",
        "scripting",
        "activeTab"
    ],
    "icons": {
        "48": "icon.png"
    },
    "name": "Full Page",
    "version": "1.0",
    "manifest_version": 3
}